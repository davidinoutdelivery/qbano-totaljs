      margin 0 0 3px
      a
        block()
        input[type=radio] 
          visibility hidden
          none()
        label 
          padding 0
          margin 0
          line-height normal
          text-align center
          height auto
          block()
          box()
          fontr()
          font(16px)
          trans(.4s)
          border-radius 0px
          padding 12px 35px 11px
