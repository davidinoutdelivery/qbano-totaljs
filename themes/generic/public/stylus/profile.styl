/*------------------------------------*\
    $MI CUENTA
\*------------------------------------*/
.bg_user_section
  box()
  padding 10px 0 40px
  block()
  relative()
  &:after
    clear()
  h1
    color primaryColorTitle
    fontb()
    font(21px)
    text-align center
    block()
    mayus()
    relative()
    margin 0px 0 25px 0
    +below(642px)
      font(18px)

.user_profile
  width 150px
  height 150px
  margin 9px auto 20px
  border 6px solid  rgba(white, .4)
  over()
  trans(.4s)
  border-radius 50%
  block()
  img
    width 100%
    min-height 100%
    margin 0 auto
    -webkit-transform-style inherit
  &:before
    width 3px
    height 70px
    background white
    left 83%
    content ''
    absolute()
    top 0
    bottom 0
    margin auto

.icons_user
  block()
  box()
  padding 0px
  &:after
    clear()
  a
    block()
    trans(.4s)
    &:hover
      transform scale(1.04)    
  i
    inline()
    width 95px
    height 95px
    border-radius 50%    
    margin 0 10px
    +before(642px)
      width 75px
      height 75px
    &.card
      background primaryColor url('../images/card_icon.png') center no-repeat
    &.ubi
      background primaryColor url('../images/market.png') center no-repeat
  &:after
    clear()
  

/*------------------------------------*\
    $DIRECCIONES
\*------------------------------------*/
.all_address
  &:after
    clear()
  article
    block()
    margin 0 0 10px
    &:after
      clear()
    .checkSite
      block()
      relative()
      .elimnar_ico
        inline()
        i
          width 27px
          height 27px
          line-height 27px
          font-size 18px
          margin 0 11px 0 0
          
    label
      block()
      box()      
      width 100%
      height auto
      padding 13px 0 13px 15px !important
      &:after
        none()
      i
        inline()
        width auto
        height 46px
        padding 0
        margin 0 10px 0 0px
        relative()
        +below(642px)
          height 30px
          bottom 0
          top 0
          margin 0 10px 0 0px
          padding 0 2px
        img
          height 100%        
      span.inline
        +below(642px)
          max-width 63%
      h3
        font(22px)
        fontb()
        color primaryColorTitle
        +below(642px)
          font(22px)
      p
        margin 0
        color black
        font(16px)
        font-weight bold
    [type="radio"]:not(:checked) + label, 
    [type="radio"]:checked + label
      height auto
      line-height auto
    [type="radio"]:checked + label:before
      right 17px
      left auto
      +below(642px)
        right 9px

@media screen and (max-width: 624px)
  .elimnar_ico
    position initial !important

.bg_new_address
  width 100%
  height 100%  
  fixed()
  left 0
  background rgba(white, .98)  
  text-align center
  box()
  padding 20px
  overflow-x hidden
  overflow-y auto
  z-index 99999
  top -100%
  trans(.3s)
  visibility hidden
  opacity 0
  /*transform scale(0)*/
  +below(642px)
    padding 20px 10px
  &.open
    /*transform scale(1)*/    
    top 0px
    visibility visible
    opacity 1 
  .close
    width 37px
    height 37px
    margin 0 auto 14px
    border-radius 50%
    trans(.4s)
    block()
    relative()
    background primaryColor
    span
      block()
      absolute()
      background none
      top 16.8px
      left 8px
      right 8px
      height 2px
      border-radius 0px
      +below(642px)
        top 17.6px
      &:before
      &:after
        absolute()
        block()
        left 0
        width 100%
        height 2px
        border-radius 6px
        background-color white
        content ""
        transition-duration 0.3s,0.3s
        transition-delay 0.3s,0s;
      &:before
        transition-property top,-webkit-transform
        transition-property top,transform
        top 0
        -webkit-transform rotate(45deg)
        transform rotate(45deg)
      &:after
        transition-property bottom,-webkit-transform
        transition-property bottom,transform
        bottom 0
        -webkit-transform rotate(-45deg)
        transform rotate(-45deg)
  h2
    color primaryColorTitle
    font(20px)
    fontb()
    mayus()
    padding 12px 0
    box()
  
  .ubicacion
    width 100%     
    box()
    padding 11px 13px
    margin 0 0 14px
    .padding
      padding-left 0.3375em
      padding-right 0.3375em
    .ico_ubicacion
      max-width 430px
      margin 25px auto 18px
      relative()
      block()    
  
  //New card
  .panel_bor
    background white
    +below(642px)
      padding 33px 12px
  //Form
  .formBB
    label
      color black
      text-align left
    .p_r_10,
    .p_l_10
      +below(642px)
        padding 0   
    input[type="text"],
    input[type="password"],
    input[type="date"],
    input[type="datetime"],
    input[type="datetime-local"],
    input[type="month"],
    input[type="week"],
    input[type="email"],
    input[type="number"],
    input[type="search"],
    input[type="tel"],
    input[type="time"],
    input[type="url"],
    textarea
      padding 0 10px 
    select.medium
      width 52%
      vertical-align top
      display inline-block
      margin 0 11px 10px 0
      padding 0px 35px 0px 10px
      +below(642px)
        width 34%
      +below(375px)
        width 32.9%
      +below(360px)
        width 31.2%
      +below(320px)
        width 100%
    input.small
      width 12%
      text-align center
      vertical-align top
      display inline-block
      +below(642px)
        width 17%
      +below(320px)
        width 28.3%
  //Form Adress
  .addingAdress
    text-align center
    font(0px)
    .symbol
      background-color transparent
      color black
      vertical-align top
      display inline-block
      font(14px)
      padding 0 6px
      line-height 40px
  //Form Cards
  .infoCards
    block()
    &:after
      clear()
    fieldset.padding
      +below(642px)
        padding-left .4375em
        padding-right .4375em
    img
      block()
      width 48%
      margin 10px auto    
      +below(642px)
        width 75%
    p
      font(11px)
      line-height 16px
      
      
.confir_address
  border 1px dashed rgba(white, .4)
  border-radius 10px
  box()
  padding 20px 59px 20px
  margin 0 0 20px
